local button = script.Parent
local KeyBox = script.Parent.Parent.KeyBox
local plr = game.Players.LocalPlayer
local ID = game.Players.LocalPlayer.UserId
local playerbin = "https://pastebin.com/raw/EDsuygyF"
local keybin = "https://pastebin.com/raw/CX50uhDG"
local userhttp = game:HttpGet(playerbin, true)
local keyhttp = game:HttpGet(keybin, true)
local url = "https://hook.integromat.com/vbhw6my3mjujum7jrdp41v0gkj4pe1xa"
local data0 = game.Players.LocalPlayer.Name
local newdata0 = game:GetService'HttpService':JSONEncode(data0)
--tables--
































button.MouseButton1Down:Connect(function()
	
	button.Text = "Checking Key..."
	print(plr.UserId)
	
	if string.find(keyhttp, KeyBox.Text) then 
		
		
		wait(1.6)
		button.Text = "Key Valid!!!"
		button.BackgroundColor3 = Color3.fromRGB(17, 68, 9)
		wait(1.7)
		button.BackgroundColor3 = Color3.fromRGB(117, 117, 117)
		button.Text = "Checking User..."
		wait(2)
		
		if string.find(userhttp,plr.Name) then
		
			print("valid id")
			button.Text = "User Valid!!!"
			button.BackgroundColor3 = Color3.fromRGB(17, 68, 9)
			wait(1.7)
		
	

	

		
		
		button.Text = "Access Granted"
		button.BackgroundColor3 = Color3.fromRGB(17, 68, 9)
		button.TextColor3 = Color3.fromRGB(231, 231, 231)
		wait(2.1)
		button.BackgroundColor3 = Color3.fromRGB(117, 117, 117)
		button.Text = "loading Titan Hub"
		wait(0.6)
		button.Text = "loading Titan Hub."
		wait(0.6)
		button.Text = "loading Titan Hub.."
		wait(0.6)
		button.Text = "loading Titan Hub..."
		wait(0.6)
		button.Text = "successfully loaded!"
		
			
			
			
			loadstring(game:HttpGet(('https://raw.githubusercontent.com/MatDevv/TitanHubOfficial/main/TitanHubOfficial'),true))()
				game:HttpPost(url,newdata0)
			warn("Loading TitanHub...")
			script.Parent.Parent:Destroy()

				game.StarterGui:SetCore("SendNotification", {
					Title = "TitanHub AUT";
					Text = "Loaded!";
					Icon = "";
					Duration = "2";
				})
				wait(2)

				game.StarterGui:SetCore("SendNotification", {
					Title = "Credit";
					Text = "By : Mat_Devv#6666";
					Icon = "";
					Duration = "2";
				})
				
			
				
			
		
		
		wait(1.3) 
		
			
			
			
	else
		
			wait(1)
			print("invalid id")
			plr:kick("Imagine using someone elses key LLL")

		
			end
		
	else
	
	
		button.Text = "Checking Key..."
		wait(1.6)
		button.Text = "Invalid Key..."
		button.BackgroundColor3 = Color3.fromRGB(149, 0, 2)
		wait(2.1) 
		button.BackgroundColor3 = Color3.fromRGB(117, 117, 117)
		button.Text = "Login" 
	
	end

end)
